{
  "name": "tpservice",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && bun build && bun start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation bun --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "migrate": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload migrate",
    "migrate:status": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload migrate:status",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm bun --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "clean": "rm -rf .next && rm -rf node_modules"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.5.0",
    "@payloadcms/email-resend": "^3.5.0",
    "@payloadcms/live-preview-react": "3.5.0",
    "@payloadcms/next": "3.5.0",
    "@payloadcms/payload-cloud": "3.5.0",
    "@payloadcms/plugin-cloud-storage": "3.5.0",
    "@payloadcms/plugin-form-builder": "3.5.0",
    "@payloadcms/plugin-nested-docs": "3.5.0",
    "@payloadcms/plugin-redirects": "3.5.0",
    "@payloadcms/plugin-search": "3.5.0",
    "@payloadcms/plugin-seo": "3.5.0",
    "@payloadcms/richtext-lexical": "^3.5.0",
    "@payloadcms/storage-s3": "3.5.0",
    "@payloadcms/storage-uploadthing": "3.5.0",
    "@payloadcms/ui": "3.5.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@supabase/supabase-js": "^2.45.6",
    "@tailwindcss/postcss": "^4.0.0-beta.2",
    "babel-plugin-react-compiler": "19.0.0-beta-8a03594-20241020",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "embla-carousel-react": "^8.3.1",
    "framer-motion": "12.0.0-alpha.1",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "jsonwebtoken": "9.0.2",
    "lexical": "0.20.0",
    "lucide-react": "^0.378.0",
    "next": "15.0.4",
    "payload": "3.5.0",
    "payload-admin-bar": "^1.0.6",
    "pg": "8.11.3",
    "pino": "9.5.0",
    "pino-pretty": "13.0.0",
    "postgres": "^3.4.5",
    "prism-react-renderer": "^2.3.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
