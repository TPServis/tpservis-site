    "@types/escape-html": "^1.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "22.5.4",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.13.0",
    "eslint-config-next": "15.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "tailwindcss": "^4.0.0-beta.2",
    "tsx": "^4.19.1",
    "typescript": "5.6.3",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
